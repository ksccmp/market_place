<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="market.place.dto.Product" alias="product"/>
		<typeAlias type="market.place.dto.User" alias="user"/>
		<typeAlias type="market.place.dto.Purchase" alias="purchase"/>
		<typeAlias type="market.place.dto.MyProduct" alias="myproduct"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="spring/mapper/productmapper.xml" />
		<mapper resource="spring/mapper/usermapper.xml" />
		<mapper resource="spring/mapper/purchasemapper.xml"/>
	</mappers>
</configuration>